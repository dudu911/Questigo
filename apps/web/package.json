{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "pnpm build:deps && next dev",
    "dev:turbo": "pnpm build:deps && next dev --turbopack",
    "build": "pnpm build:deps && next build",
    "build:deps": "pnpm --filter '@questigo/shared' --filter '@questigo/ui' build",
    "start": "next start",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@questigo/shared": "workspace:*",
    "@questigo/ui": "workspace:*",
    "next": "15.3.5",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.3.5",
    "postcss": "^8",
    "tailwindcss": "^3.4.0",
    "typescript": "^5"
  }
}
